{
    "name"             : "mamuz/mm-web",
    "description"      : "Blog about practices, principles and patterns of web-development with PHP",
    "license"          : "proprietary",
    "keywords"         : ["mamuz"],
    "authors"          : [
        {
            "name" : "Marco Muths",
            "email": "muzzi_is@web.de"
        }
    ],
    "repositories"     : [
        {
            "type": "composer",
            "url" : "http://packages.zendframework.com/"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable"    : true,
    "require"          : {
        "php"                        : ">=5.4",
        "zendframework/zendframework": "~2.3",
        "mamuz/mamuz-blog": "1.*",
        "mamuz/mamuz-contact"        : "0.*",
        "mamuz/mamuz-content-manager": "0.*"
    },
    "require-dev"      : {
        "zendframework/zend-developer-tools": "dev-master",
        "satooshi/php-coveralls"     : "0.6.*",
        "phpunit/phpunit"            : "4.*",
        "mockery/mockery"                   : "0.9.*",
        "pdepend/pdepend"                   : "2.0.*",
        "phpmd/phpmd"                : "2.2.*",
        "squizlabs/php_codesniffer"  : "2.2.*",
        "sensiolabs/security-checker": "2.*"
    }
}
