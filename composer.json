{
    "name"             : "mamuz/mm-web",
    "description"      : "Practices, principles and patterns about web-development with PHP",
    "keywords"         : ["mamuz"],
    "authors"          : [
        {
            "name" : "Marco Muths",
            "email": "muzzi_is@web.de"
        }
    ],
    "repositories"     : [
        {
            "type": "composer",
            "url" : "http://packages.zendframework.com/"
        }
    ],
    "minimum-stability": "dev",
    "require"          : {
        "php"                                : ">=5.4",
        "zendframework/zendframework"        : "~2.3.1",
        "doctrine/doctrine-orm-module"       : "0.*",
        "maglnet/magl-markdown"              : "1.*",
        "neilime/zf2-twb-bundle"             : "2.*@stable",
        "zendframework/zendservice-recaptcha": "~2.0.1",
        "hashids/hashids"                    : "*"
    },
    "require-dev"      : {
        "satooshi/php-coveralls"   : "dev-master",
        "phpunit/phpunit"          : "4.1.*",
        "mockery/mockery"          : "0.9.*",
        "pdepend/pdepend"          : "1.1.*",
        "phpmd/phpmd"              : "1.4.*",
        "squizlabs/php_codesniffer": "2.0.*@dev",
        "sebastian/phpcpd"         : "*",
        "phploc/phploc"            : "*",
        "mayflower/php-codebrowser": "~1.1"
    }
}
