{
    "name"             : "mamuz/mm-web",
    "description"      : "Practices, principles and patterns about web-development with PHP",
    "license"          : "proprietary",
    "keywords"         : ["mamuz"],
    "authors"          : [
        {
            "name" : "Marco Muths",
            "email": "muzzi_is@web.de"
        }
    ],
    "repositories"     : [
        {
            "type": "composer",
            "url" : "http://packages.zendframework.com/"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable"    : true,
    "require"          : {
        "php"                        : ">=5.4",
        "zendframework/zendframework": "~2.3.1",
        "mamuz/mamuz-blog"           : "1.*",
        "mamuz/mamuz-contact"        : "0.*",
        "mamuz/mamuz-content-manager": "0.*"
    },
    "require-dev"      : {
        "zendframework/zend-developer-tools": "dev-master",
        "ocramius/ocra-service-manager": "2.1.*",
        "doctrine/doctrine-orm-module" : "dev-master",
        "satooshi/php-coveralls"            : "dev-master",
        "phpunit/phpunit"              : "4.2.*",
        "mockery/mockery"                   : "0.9.*",
        "pdepend/pdepend"                   : "2.0.*",
        "phpmd/phpmd"                       : "2.0.*",
        "squizlabs/php_codesniffer"         : "2.0.*@dev",
        "sensiolabs/security-checker"       : "1.*"
    }
}
