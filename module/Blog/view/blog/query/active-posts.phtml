<ul>
    <?php
    foreach ($this->collection as $entity) {
        /** @var \Blog\Entity\Post $entity */
        echo '<li>' . $entity->getTitle();
        $tags = $entity->getTags();
        echo '<ul>';
        foreach ($tags as $tag) {
            echo '<li>' . $tag->getName() . '</li>';
        }
        echo '</ul></li>';
    }
    ?>
</ul>
